{
  "name": "readit-exercise",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel json-server start",
    "build": "vite build",
    "preview": "npm-run-all --parallel json-server-test start-preview",
    "prepreview": "npm run build",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore src",
    "start": "vite",
    "start-preview": "vite preview",
    "json-server": "json-server ./db/readit.json --routes ./db/routes.json",
    "json-server-test": "json-server ./db/readit.json --routes ./db/routes.json --quiet",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress open --e2e --browser electron'",
    "test:e2e-headless": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "vue": "^3.3.4",
    "vue-router": "^4.2.1",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.2.0",
    "cypress": "^12.14.0",
    "eslint": "^8.42.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-vue": "^7.20.0",
    "json-server": "^0.17.3",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^2.0.0",
    "vite": "^3.2.7",
    "vue-eslint-parser": "^9.3.0"
  }
}
